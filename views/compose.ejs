<%- include("partials/header") %>

    <h1>Compose a new post!</h1>
   <form action="/compose" method="post">
       <label for="title">Title</label>
       <input type="text" class="form-control" id="title" name="postTitle" >
       <label for="post">Post</label>
       <textarea name="postContent" id="post" class="form-control" cols="30" rows="10"></textarea>
       <br>
       <button class="btn btn-info " type="input">Publish</button>
   </form>

<%- include("partials/footer") %>