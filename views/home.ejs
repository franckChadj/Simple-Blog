<%- include("partials/header") %>

    <h1>Home</h1>
    <p><%=  textHome %> </p>

    <% postSet.forEach(function(post) { %>
        <p>
            <em><h2><%= post.title  %></h2></em>
            <p><%= post.content.substring(0,100) + "..."  %> <a href="/posts/<%= post._id  %>">Read More</a> </p>
        </p>
    <% }) %>
    
<%- include("partials/footer") %>